"Viewing Party Application (Andra and Hady)"

<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <%= link_to "Log In", login_path %>
<% end %>

<!-- <%= link_to "Log In", login_path, method: :get %> -->
<br>
<%= button_to "Create New User", "/register", method: :get %>

<div id="existing_users">
  <h2> Existing Users and Their Dashboards </h2>
  <%@users.each do |user|%>
  <p><%=user.name%> <%=link_to "#{user.name}'s Dashboard", "/users/#{user.id}"%> </p>
  <%end%>
</div>
